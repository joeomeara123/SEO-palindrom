{
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/chatgpt-vs-claude",
      "destination": "/chatgpt-vs-claude.html"
    },
    {
      "source": "/chatgpt-vs-copilot",
      "destination": "/chatgpt-vs-copilot.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}