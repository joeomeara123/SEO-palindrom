{
  "version": 2,
  "name": "palindrom-seo-prototype",
  "builds": [
    {
      "src": "**/*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/chatgpt-vs-claude",
      "dest": "/chatgpt-vs-claude.html"
    },
    {
      "src": "/chatgpt-vs-copilot",
      "dest": "/chatgpt-vs-copilot.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}